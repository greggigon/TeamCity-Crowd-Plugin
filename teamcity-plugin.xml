<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>TeamCityCrowdPlugin</name>
        <display-name>TeamCity Crowd Plugin</display-name>
        <version>@Version@</version>
        <download-url>@VendorURL@</download-url>
        <email></email>
        <vendor>
            <name>@VendorName@</name>
            <url>@VendorURL@</url>
            <email>@VendorEmail@</email>
        </vendor>
    </info>
    <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
</teamcity-plugin>

